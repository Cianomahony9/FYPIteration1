<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>create</servlet-name>
        <servlet-class>create</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createFamily1</servlet-name>
        <servlet-class>createFamily1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertFamilyServ</servlet-name>
        <servlet-class>insertFamilyServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>retrieveFamily</servlet-name>
        <servlet-class>retrieveFamily</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchBike</servlet-name>
        <servlet-class>searchBike</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>create</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createFamily1</servlet-name>
        <url-pattern>/createFamily1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertFamilyServ</servlet-name>
        <url-pattern>/insertFamilyServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>retrieveFamily</servlet-name>
        <url-pattern>/retrieveFamily</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchBike</servlet-name>
        <url-pattern>/searchBike</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
